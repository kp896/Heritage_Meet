<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.o2meet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/flogo1_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.O2Meet">
        <activity android:name=".introductoryContactPage" />
        <activity android:name=".introductoryPage" />
        <activity android:name=".aboutUs" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".profile" />
        <activity android:name=".help" />
        <activity android:name=".LoginActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />


        <activity
            android:configChanges="keyboard|orientation"
            android:launchMode="singleTask"
            android:name="com.dropbox.core.android.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:scheme="db-bdu13ij0c1pt9kx" />
            </intent-filter>
        </activity>

    </application>

</manifest>